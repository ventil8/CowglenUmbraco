  <package id="6" version="2.5.7" url="" name="ImageGen" folder="5e9eb1f0-c592-4784-8e13-5f87f3259c86" packagepath="" repositoryGuid="65194810-1f85-11dd-bd0b-0800200c9a66" packageGuid="cbc37b15-4a53-4c3f-b8a0-e49543d84192" hasUpdate="False" enableSkins="False" skinRepoGuid="00000000-0000-0000-0000-000000000000">
